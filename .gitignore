frontend/.output/
frontend/.vercel/
frontend/.netlify/

# 編輯器和 IDE
frontend/.vscode/
frontend/.idea/
frontend/*.swp
frontend/*.swo
frontend/*~

# 作業系統檔案
frontend/.DS_Store
frontend/Thumbs.db

# 測試覆蓋率
frontend/coverage/
frontend/.nyc_output/

# ESLint 快取
frontend/.eslintcache

